{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "semicolon": false,
    "arrow-parens": false,
    "trailing-comma": false,
    "quotemark": [true],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-unused-expression": false,
    "no-var-requires": false
  },
  "rulesDirectory": []
}
